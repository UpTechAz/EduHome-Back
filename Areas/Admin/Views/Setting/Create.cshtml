@model WebApplication2.Models.Setting
<div class="col-lg-6">
    <div class="card">
        <div class="card-body">
            <div class="card-title">Add Information</div>
            <hr>
            <form method="post" enctype="multipart/form-data" asp-action="create" asp-controller="Setting" asp-area="admin">
                <div class="form-group">
                    <label asp-for="Key">Key</label>
                    <input asp-for="Key" class="form-control">
                    <span class="text-danger" asp-validation-for="Key"></span>
                </div>
                <div class="form-group">
                    <label asp-for="Value">Value</label>
                    <input asp-for="Value" class="form-control">
                    <span class="text-danger" asp-validation-for="Value"></span>
                </div>
                <div class="form-group">
                    <button class="btn btn-light px-4">Submit</button>
                </div>
            </form>
        </div>
    </div>
</div>
